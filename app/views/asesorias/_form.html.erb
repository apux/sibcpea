<%= form_for(@asesoria) do |f| %>
  <%= render 'shared/error_messages', :target => @asesoria %>

  <table class="form">
    <thead>
      <tr>
        <td colspan="2">Datos del Alumno</td>
      </tr>
    </thead>
    <tfoot>
      <tr>
        <td colspan="2">
          <div class="actions">
            <%= f.submit %>
          </div>
        </td>
      </tr>
    </tfoot>
    <tbody>
      <tr>
        <td class="name">
          <%= f.label :fecha %>
        </td>
        <td class="input">
          <%= f.date_select :fecha %>
        </td>
      </tr>
      <tr>
        <td class="name">
          <%= f.label :nombre %>
        </td>
        <td class="input">
          <%= f.text_field :nombre %>
        </td>
      </tr>
      <tr>
        <td class="name">
          <%= f.label :codigo %>
        </td>
        <td class="input">
          <%= f.text_field :codigo %>
        </td>
      </tr>
      <tr>
        <td class="name">
          <%= f.label :tema %>
        </td>
        <td class="input">
          <%= f.text_field :tema %>
        </td>
      </tr>
    </tbody>
  </table>
<% end %>
